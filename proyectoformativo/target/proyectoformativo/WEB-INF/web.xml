<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  <display-name>Archetype Created Web Application</display-name>
   <welcome-file-list>
  <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
<servlet>
    <servlet-name>controller.empleadoController</servlet-name>
    <servlet-class>controller.empleadoController</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>controller.empleadoController</servlet-name>
    <url-pattern>/empleado</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>controller.horarioReController</servlet-name>
    <servlet-class>controller.horarioReController</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>controller.horarioReController</servlet-name>
    <url-pattern>/horarioRegistrado</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>controller.horarioAsigController</servlet-name>
    <servlet-class>controller.horarioAsigController</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>controller.horarioAsigController</servlet-name>
    <url-pattern>/horarioAsig</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>controller.soporteController</servlet-name>
    <servlet-class>controller.soporteController</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>controller.soporteController</servlet-name>
    <url-pattern>/soporte</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>controller.login</servlet-name>
    <servlet-class>controller.login</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>controller.login</servlet-name>
    <url-pattern>/login</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>controller.logout</servlet-name>
    <servlet-class>controller.logout</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>controller.logout</servlet-name>
    <url-pattern>/logout</url-pattern>
</servlet-mapping>
</web-app>
